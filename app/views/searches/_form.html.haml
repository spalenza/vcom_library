= simple_form_for @search, html: { multipart: true }, remote: true do |f|
  = f.input :name
  = f.label :user_id
  = f.text_field :user_id
  = f.label :element_tokens, "Elements"
  = f.text_field :element_tokens, name: 'search[element_tokens][]'
  %br
  = f.button :submit, "Buscar"

#close
  %i.icon-chevron-up.icon-white
%br
%br